from pathlib import Path
import re, csv


filepath= Path.cwd()/"profit-and-loss-usd (1).csv"
#print(filepath.exists())

emptylist=[]
with filepath.open(mode= 'r', encoding= 'UTF-8', newline="") as file1:
        reading = csv.reader(file1)
        next(reading)
        for line in reading:
                emptylist.append(line[4])
        print(emptylist)

